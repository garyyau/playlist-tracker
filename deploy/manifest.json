{
	"name": "Playlist Tracker Ex",
	"description": "This extension will manage your Playlist.",
	"version": "1.0",
	"manifest_version": 2,

	"icons": {
		"128": "images/128-icon.png"
	},

	"browser_action": {
		"default_icon": "images/icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"tabs",
		"storage",
		"http://*/",
		"https://*/"
	 ]
}
