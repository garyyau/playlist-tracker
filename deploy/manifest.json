{
	"name": "Playlist Tracker Ex",
	"description": "This extension will manage your Playlist.",
	"version": "1.0",
	"manifest_version": 2,

	"browser_action": {
		"default_icon": "images/icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"http://*/",
		"https://*/",
	 ]
}
